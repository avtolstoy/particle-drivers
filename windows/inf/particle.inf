[DeviceList.NTamd64] 
; Photon
%DEVICE_DESCRIPTION_PHOTON_1%=DriverInstall, USB\VID_2B04&PID_C006&REV_0200&MI_00
%DEVICE_DESCRIPTION_PHOTON_2%=DriverInstall, USB\VID_2B04&PID_C006&REV_0200&MI_03

